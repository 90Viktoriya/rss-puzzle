{
  "root": true,
  "parser": "@typescript-eslint/parser",
  "plugins": [
    "import",
    "@typescript-eslint",
    "prettier"
  ],
  "extends": [
    "airbnb-base",
    "airbnb-typescript/base",
    "eslint:recommended",
    "plugin:@typescript-eslint/eslint-recommended",
    "plugin:@typescript-eslint/recommended",
    "prettier"
  ],
  "parserOptions": {
    "sourceType": "module",
    "project": ["tsconfig.json"]
  },
  "rules": {
    "no-console": 0,
    "@typescript-eslint/no-explicit-any": "error",
    "prettier/prettier": 2
},

"noInlineConfig": true
}